# Environment (development, production, test)
NODE_ENV=development

# Database configuration
DATABASE_URL=sqlite:database.sqlite

# OpenAI API configuration
OPENAI_API_KEY=your_openai_api_key_here

# Agent response interval in milliseconds (default: 1000)
# This controls how often agents in active conversations will respond to each other
AGENT_RESPONSE_INTERVAL=1000

# Goal check interval in milliseconds (default: 50)
# This controls how often the goal is checked
GOAL_CHECK_INTERVAL=50 